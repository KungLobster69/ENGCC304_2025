    "#ระบบ บวกเลข\n",
    "try:\n",
    "    num = int(input(\"Enter number: \"))\n",
    "    total = 0\n",
    "    for i in range(1, num + 1):\n",
    "        total = total + i\n",
    "    print(\"Sum is: \", total)\n",
    "\n",
    "except ValueError :\n",
    "    print(\"ข้มูลไม่ถูกต้อง\")"
   ]
  }
