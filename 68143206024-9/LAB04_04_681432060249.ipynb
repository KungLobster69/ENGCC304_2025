    "# Workshop 04: โปรแกรมหาผลรวม 1 ถึง N\n",
    "\n",
    "n = input(input(\"ใส่จำนวน N:\"))\n",
    "total = 0\n",
    "for i in range(1, int(n) + 1):\n",
    "    total += i\n",
    "print(f\"ผลรวมของ 1 ถึง {n} คือ {total}\")"
   ]
