    "score = float(input(\"โปรดใส่คะแนนของคุณ\"))\n",
    "if score <50 :\n",
    "    grade = \"F\"\n",
    "elif 70 >score >= 60 :\n",
    "    grade = \"D\"\n",
    "elif 80 >score >= 70 :\n",
    "    grade = \"C\"\n",
    "elif 90 >score >= 80 :\n",
    "    grade = \"B\"\n",
    "elif score >= 90 :\n",
    "    grade = \"A\"\n",
    "print(f\"คะแนน{score}ได้เกรด{grade}\")"
   ]
  }
