    "\n",
    "answer = random.randint(1, 100)\n",
    "while True:\n",
    "    guess = int(input(\"ทายตัวเลข (1-100): \"))\n",
    "    if guess < answer:\n",
    "        print(\"น้อยไป\")\n",
    "    elif guess > answer:\n",
    "        print(\"มากไป\")\n",
    "    else:\n",
